{
  "name": "ventio",
  "private": true,
  "scripts": {
    "dev": "pnpm -w run compose:up && pnpm -w --parallel -r run dev",
    "compose:up": "docker compose -f docker-compose.yml up -d",
    "compose:down": "docker compose -f docker-compose.yml down -v"
  },
  "dependencies": {
    "@vendure/graphiql-plugin": "^3.4.1",
    "bcrypt": "npm:bcryptjs@^3.0.2"
  },
  "pnpm": {
    "overrides": {
      "bcrypt": "npm:bcryptjs@^3.0.2"
    }
  }
}
